@using Dj_ReedRussel.Models;
@model dynamic
@{
    ViewBag.Title = "Video-Singles";
    Layout = "~/Views/Shared/Menu_Footer.cshtml";
}


<!--page starts-->
<section>
    <div class="back">
        <div class="page-name">
            <h3> Lorem ipsum dolor sit amet.</h3>
            <h2 class="title">Videos</h2>
        </div>
    </div>
</section>
<!--page ends-->
<!--page content starts-->
<section class="modal-galery" style="background:#04010d;">
    <div class="container">
        <div class="row">
            <div class="forheader">
                <span>music SECTION</span>
                <h3 style="margin-bottom: 83px;margin-top: 8px;">VIDEOS</h3>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="list">
                @foreach (video item in Model.videos)
                {
                    <a href="@item.link" class="gallery1">
                        <img src="~/Uploads/@item.cover" alt="mabas sbm95" />
                        <i class="fa fa-play-circle"></i>
                    </a>
                }
                
               
            </div>
        </div>
    </div>
    <div class="modal">
        <div class="modal_inner">
            <img class="close" src="~/Public/img/close.png" width="32">
            <img class="left" src="~/Public/img/arrow-left.png" width="32">
            <img class="right" src="~/Public/img/arrow-right.png" width="32">
        </div>
    </div>
</section>
<!--page content ends-->
<!--festival starts-->
<section class="festival list">
    <p>
        MUSIC
        <a href="https://www.youtube.com/embed/feA64wXhbjo?list=RDGMEMYH9CUrFO7CfLJpaD7UR85wVMAd6SxduZfis" class="gallery1">
            <i class="fa fa-play-circle"></i>
        </a> festival
    </p>
    <div class="modal">
        <div class="modal_inner">
            <img class="close" src="~/Public/img/close.png" width="32">
            <img class="left" style="display: none" src="~/Public/img/arrow-left.png" width="32">
            <img class="right" style="display: none" src="~/Public/img/arrow-right.png" width="32">
        </div>
    </div>
</section>
<!--festival ends-->

